
 <td width="8%" align="center" valign="top">
    <% name = value.element.name %>
    <% amount = "amount_" + name %>
    <% @enabled ||= false %>
    <script>
      pp_slider("<%= name %>", "<%= value.value %>", <%= @enabled %>)
    </script>

    <div id="value">
        <div id="<%= name %>" style="height:200px;"></div>
        <p>
          <lable for:"<%= name %>"><%= t name %></lable>
        </p>
        <p>
            <input type="hidden" id="<%= amount %>" name="values[<%= name %>][]" readonly style="border:0; color:#f6931f; font-weight:bold;" value=<%= value.value %>>
        </p>
    </div>
 </td>
